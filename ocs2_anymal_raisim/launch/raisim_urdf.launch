<launch>
  <arg name="robot_name"/>

  <!-- Load bear description -->
  <group if="$(eval arg('robot_name') == 'bear')">
    <param name="/anymal_raisim_urdf" textfile="$(find sim2real_models)/models/anymal/anymal_bear/2019-03-12/urdf/anymal_minimal.urdf"/>
  </group>

  <!-- Load croc description -->
  <group if="$(eval arg('robot_name') == 'croc')">
    <param name="/anymal_raisim_urdf" textfile="$(find sim2real_models)/models/anymal/anymal_croc/2020-05-11/urdf/anymal_minimal.urdf"/>
  </group>

  <!-- Add others .. -->

</launch>
